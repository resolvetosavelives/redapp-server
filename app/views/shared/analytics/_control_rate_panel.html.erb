<div class="featured-section">
    <div class="featured-section-contents">
      <strong class="featured-title">
        <%= t('analytics.control_rate_title') %>
      </strong>
      <span id='control-rate' class="featured-number">
        <%= control_rate %>%
      </span>

      <%= render "shared/graph",
                 options: { id: 'control-rate-graph' },
                 column_class: 'featured-graph-column',
                 graph_data: control_rate_per_month
                     .select { |month, _| month >= (@to_time - number_of_months.months).at_beginning_of_month }
                     .map { |month, no_of_patients| [month.strftime('%b'), no_of_patients] }
                     .to_h,
                 max_value: control_rate_per_month.values.max %>
    </div>
</div>