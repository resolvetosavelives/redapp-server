<div class="featured-section">
  <strong class="featured-title" style="color: #E0B000;">
    <%= t('analytics.non_returning_hypertensive_patients') %>
  </strong>
  <span id='non-returning-hypertensive-patients-count' class="featured-number text-yellow">
    <%= non_returning_hypertensive_patients_count %>
  </span>

  <span class="featured-label">Patients</span>

  <%= render "shared/graph",
             options: { id: 'non-returning-hypertensive-patients-graph' },
             column_class: 'featured-graph-column-yellow',
             graph_data: latest_months(non_returning_hypertensive_patients_per_month, number_of_months) %>

</div>